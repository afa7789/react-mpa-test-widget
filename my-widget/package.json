    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:widget": "parcel build src/index.js --no-source-maps --dist-dir widget && cp build/static/css/*.css widget/index.css",
