  "private": true,
  "dependencies": {

    "axios": "^0.26.1",
    "babel-polyfill": "^6.26.0",
    
    "dotenv": "^16.0.0",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-responsive": "^9.0.0-beta.8",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "react-tooltip": "^4.2.21",
    "regenerator-runtime": "^0.13.9",
